pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
function _init()
    -- place the 2x2 sprite on the map
    mset(14, 5, 1)   -- top-left
    mset(14, 6, 17)  -- bottom-left
    mset(15, 5, 2)   -- top-right
    mset(15, 6, 18)  -- bottom-right
end

function _draw()
    -- clear the screen
    cls()
    
    -- render the map manually
    for y = 0, 15 do
        for x = 0, 15 do
            local sprite_id = mget(x, y)
            if sprite_id > 0 and not fget(sprite_id, 0) then
                spr(sprite_id, x * 8, y * 8)
            end
        end
    end
end
